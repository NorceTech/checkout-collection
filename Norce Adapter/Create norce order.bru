meta {
  name: Order
  type: http
  seq: 3
}

post {
  url: {{norceAdapterUrl}}/api/v1/orders
  body: json
  auth: bearer
}

headers {
  x-merchant: {{merchant}}
  x-channel: {{channel}}
}

auth:bearer {
  token: {{token}}
}

body:json {
  {
    "cartReference": {{basketId}},
    "culture": "sv-SE",
    "currency": "SEK"
  }
}

vars:post-response {
  orderId: res.body.id
}
